{
  "performance": {
    "updateInterval": {
      "default": 50,
      "highPerformance": 33,
      "powerSaver": 100,
      "adaptive": {
        "enabled": true,
        "cpuThresholds": {
          "low": 30,
          "medium": 60,
          "high": 85
        },
        "intervals": {
          "low": 33,
          "medium": 50,
          "high": 100
        }
      }
    },
    "rendering": {
      "markerUpdateBatching": true,
      "smoothAnimations": true,
      "layerCaching": true,
      "viewportCulling": true,
      "maxMarkersVisible": 1000
    },
    "memory": {
      "maxCacheSize": 50,
      "cacheCleanupInterval": 300000,
      "positionHistoryLimit": 100,
      "errorLogLimit": 50
    },
    "webview": {
      "messageQueueSize": 100,
      "maxConcurrentMessages": 10,
      "responseTimeout": 5000,
      "retryAttempts": 3,
      "retryDelay": 1000
    }
  },
  "quality": {
    "mapImageQuality": {
      "high": {
        "compression": 85,
        "format": "webp",
        "maxSize": 2048
      },
      "medium": {
        "compression": 75,
        "format": "jpeg",
        "maxSize": 1024
      },
      "low": {
        "compression": 60,
        "format": "jpeg", 
        "maxSize": 512
      }
    },
    "markerQuality": {
      "highDPI": true,
      "antiAliasing": true,
      "shadowEffects": true,
      "gradientSupport": true
    }
  },
  "debugging": {
    "enabled": false,
    "logLevel": "info",
    "performanceMetrics": false,
    "memoryMonitoring": false,
    "networkLogging": false,
    "coordinateValidation": true
  },
  "features": {
    "experimentalFeatures": {
      "webWorkers": false,
      "offscreenCanvas": false,
      "webGL": false,
      "webAssembly": false
    },
    "accessibility": {
      "highContrast": true,
      "keyboardNavigation": true,
      "screenReader": true,
      "focusIndicators": true
    }
  },
  "thresholds": {
    "performance": {
      "targetFPS": 60,
      "maxFrameTime": 16.67,
      "maxMemoryUsage": 100,
      "maxRenderTime": 10
    },
    "quality": {
      "minAccuracy": 0.95,
      "maxPositionError": 5.0,
      "maxCoordinateDeviation": 1.0
    }
  },
  "optimizations": {
    "lazy": {
      "imageLoading": true,
      "layerInitialization": true,
      "componentMounting": true
    },
    "preloading": {
      "adjacentMaps": false,
      "commonAssets": true,
      "fontPreload": true
    },
    "caching": {
      "coordinateTransforms": true,
      "mapConfigurations": true,
      "userSettings": true,
      "positionHistory": true
    }
  }
}